<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="st1" class="beans.Student">
		<property name="name" value="Nguyen Bka" />
		<property name="addr" value="HCM City" />
	</bean>
	
	<bean id="cStartEventHandler" class="event_handling.CStartEventHandler" />
	<bean id="cStopEventHandler" class="event_handling.CStopEventHandler" />
</beans>
